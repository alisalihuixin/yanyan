.qiye {
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  margin-top: 20rpx;
  padding: 20rpx 0;
  margin-bottom: 10rpx;
}

.chuanwrap image {
  width: 100rpx;
  height: 100rpx;
  float: left;
  margin: 10rpx;
  margin-left: 30rpx;
}

.chuanwrap .name {
  float: left;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #333;
}
.btn1{
  width: 80rpx;
  height:80rpx;
}
.zhuanfa{
  float: left;
  margin-left:-16rpx;
}
.picwrap{
  width: 100rpx;
  height: 100rpx;
  line-height:100rpx;
}
button{
  border: 0 !important;
}
.allpicwrap{
  width: 110rpx;
  text-align: center;
}
.picwrap button{
  line-height: 100rpx;
  width: 100%;
  height: 100%;
  text-align: center;
  position: none !important;
  border: none !important;
  background-color: transparent !important;
}
button::after{ border: none; } 
.in{
  text-align: right;
  border-radius: 10rpx;
  padding: 10rpx 20rpx;
  border:1px solid #999;
  font-size: 26rpx;
  width: 120rpx;
  height: 40rpx;
  float: right;
  margin-right: 30rpx;
  margin-top: 34rpx;
  text-align: center;
  line-height: 40rpx;
}
.header{
  width: 100%;
  text-align: center;
  line-height:100rpx;
  font-size: 30rpx;
  height: 100rpx;
  background-color: #fff;
}