/* care//myphone/myphone.wxss */
.hasphone{
  background-color: #fff;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
}

.myphone{
  margin:0 20rpx;
}
.inputtext{
  background-color: #fff;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
}
.inputtext input{
  width: 100%;
  height: 100%;
  border: 1px solid #eee;
  border-radius: 20rpx;
  text-indent: 20rpx;
}
.reset{
  width: 80%;
  border-radius: 20rpx;
  margin-left: 10%;
  margin-top: 20rpx;
  background-color: rgb(6, 96, 170);
  color: #fff;
}
.header{
  width: 100%;
  text-align: center;
  line-height:100rpx;
  font-size: 30rpx;
  height: 100rpx;
  background-color: #fff;
}